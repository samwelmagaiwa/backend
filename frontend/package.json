{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:prod": "NODE_ENV=production vue-cli-service build --modern",
    "build:analyze": "vue-cli-service build --analyze",
    "build:report": "vue-cli-service build --report",
    "build:modern": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "lint:check": "eslint --ext .js,.vue src/",
    "lint:fix": "eslint --ext .js,.vue src/ --fix",
    "clean": "node clear-cache.js",
    "clean:hard": "rimraf node_modules/.cache && rimraf dist && rimraf .eslintcache",
    "dev": "npm run clean && npm run serve",
    "dev:mobile": "vue-cli-service serve --host 0.0.0.0 --port 8080",
    "optimize:images": "node scripts/optimize-images.js",
    "analyze:bundle": "npm run build:analyze && npx webpack-bundle-analyzer dist/report.html",
    "fix-deps": "node fix-dependencies.js",
    "verify": "node verify-setup.js",
    "reset": "node reset-project.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@tailwindcss/postcss": "^4.1.11",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "autoprefixer": "^10.4.16",
    "compression-webpack-plugin": "^10.0.0",
    "cssnano": "^6.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.32",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
